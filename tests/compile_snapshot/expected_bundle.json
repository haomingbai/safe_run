{
  "firecrackerConfig": {
    "boot-source": {
      "boot_args": "console=ttyS0 reboot=k panic=1 pci=off",
      "kernel_image_path": "artifacts/vmlinux"
    },
    "drives": [],
    "machine-config": {
      "mem_size_mib": 256,
      "smt": false,
      "vcpu_count": 1
    }
  },
  "jailerPlan": {
    "enabled": true,
    "ops": [
      "prepare_jailer_context"
    ]
  },
  "cgroupPlan": {
    "enabled": true,
    "ops": [
      "set_cpu_max=100000 100000",
      "set_memory_max=256Mi"
    ]
  },
  "networkPlan": null,
  "evidencePlan": {
    "enabled": true,
    "events": [
      "compile"
    ]
  }
}
